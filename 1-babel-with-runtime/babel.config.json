{
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "edge": "17",
            "ie": "9",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          },
          "useBuiltIns": "usage",
          "corejs": "3.6.5"
        }
      ]
    ],
    "plugins": [
      ["@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": 3,
          "helpers": true,
          "regenerator": true,
          "version": "7.0.0-beta.0"
        }]
    ]
  }